    <title>Nusayba Iftekhar</title>
    <script type = "text/javascript">

      document.writeln(
        "<h1>Nusayba Iftekhar</h1>"
      );

    </script>
  </head><body></body>
</html>


2. <html>
  <head>
    <meta charset = "utf-8">
    <title> A First Program in JavaScript</title>
    <script type = "text/javascript">

      document.writeln(
        "<h1>Welcome to JavaScript Programming!</h1>"
      );

    </script>
  </head><body></body>
</html>

3.<html>
  <head>
    <meta charset = "utf-8">
    <title>Nusayba Iftekhar</title>
    <script type = "text/javascript">

      var hours;
      var rate;
      var hourCalculation;
      var rateCalculation;
      var salary;

      hours = window.prompt("Enter hours worked");
      rate = window.prompt("Enter the rate per hour");

      hourCalculation = parseInt(hours);
      rateCalculation = parseInt(rate);

      salary = hourCalculation * rateCalculation;

      document.writeln("<h1>The salary is " + salary + ".</h1>");
    </script>
  </head><body></body>
</html>

4.<html>
  <head>
    <meta charset = "utf-8">
    <title>Nusayba Iftekhar</title>
    <script type = "text/javascript">

      var length;
      var width;
      var lengthCalculation;
      var widthCalculation;
      var areaRect;

      length = window.prompt("Enter length of rectangle");
      width = window.prompt("Enter width of rectangle");

      lengthCalculation = parseInt(length);
      widthCalculation = parseInt(width);

      areaRect = lengthCalculation * widthCalculation;

      document.writeln("<h1>The area of the rectangle is " + areaRect + "ft.</h1>");
    </script>
  </head><body></body>
</html>

5.<html>
  <head>
    <meta charset = "utf-8">
    <title> AreaCircle </title>
    <script type = "text/javascript">

      var radius;
      var radiusInput;
      var areaCircle;

      radius = window.prompt("Enter radius of circle");

      radiusInput = parseInt(radius);

      areaCircle = 3.14 * radiusInput * radiusInput;

      document.writeln("<h1>The area of the circle is " + areaCircle + " ft squared.</h1>");
    </script>
  </head><body></body>
</html>

